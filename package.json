{
  "name": "caiu",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_grid": "ng build --prod grid",
    "build_lib": "ng build library --prod",
    "npm_pack": "npm_pack_grid && npm_pack_lib",
    "npm_pack_grid": "cd dist/grid && npm pack",
    "npm_pack_lib": "cd dist/library && npm pack",
    "package_grid": "npm run build_grid && npm run npm_pack_grid",
    "package_lib": "npm run build_lib && npm run npm_pack_lib",
    "package": "npm run package_grid && npm run package_lib",
    "publish_grid": "npm run package_grid && cd dist/grid && npm publish",
    "publish_lib": "npm run package_lib && cd dist/library && npm publish",
    "publish": "npm run publish_grid && npm run publish_lib"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^10.2.0",
    "@angular/animations": "^10.2.3",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^10.2.3",
    "@angular/compiler": "^10.2.3",
    "@angular/core": "^10.2.3",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^10.2.3",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^10.2.3",
    "@angular/platform-browser-dynamic": "^10.2.3",
    "@angular/router": "^10.2.3",
    "@caiu/library": "^10.3.6",
    "@microsoft/signalr": "^3.1.10",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "ngx-color-picker": "^10.1.0",
    "rxjs": "~6.5.4",
    "tinymce": "^5.6.2",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-ng-packagr": "~0.1000.5",
    "ng-packagr": "^10.0.0",
    "tsickle": ">=0.38.1",
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/cli": "^10.2.0",
    "@angular/compiler-cli": "^10.2.3",
    "@angular/language-service": "^10.2.3",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.9.5",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.8.1",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7"
  }
}
