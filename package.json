{
  "name": "caiu",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_grid": "ng build --prod grid",
    "build_lib": "ng build --prod library",
    "npm_pack": "npm_pack_grid && npm_pack_lib",
    "npm_pack_grid": "cd dist/grid && npm pack",
    "npm_pack_lib": "cd dist/library && npm pack",
    "package_grid": "npm run build_grid && npm run npm_pack_grid",
    "package_lib": "npm run build_lib && npm run npm_pack_lib",
    "package": "npm run package_grid && npm run package_lib",
    "publish_grid": "npm run package_grid && cd dist/grid && npm publish",
    "publish_lib": "npm run package_lib && cd dist/library && npm publish",
    "publish": "npm run publish_grid && npm run publish_lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.2.15",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@caiu/library": "^8.3.17",
    "@ngrx/effects": "^8.5.2",
    "@ngrx/store": "^8.5.2",
    "caniuse-lite": "1.0.30000997",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.4.0",
    "tinymce": "^5.1.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-ng-packagr": "~0.800.2",
    "ng-packagr": "~5.3.0",
    "tsickle": ">=0.35.0",
    "tslib": "^1.10.0",
    "@angular-devkit/build-angular": "^0.800.4",
    "@angular/cli": "^8.3.19",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
