{
  "name": "caiu",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_grid": "ng build --prod grid",
    "build_lib": "ng build library --prod",
    "npm_pack": "npm_pack_grid && npm_pack_lib",
    "npm_pack_grid": "cd dist/grid && npm pack",
    "npm_pack_lib": "cd dist/library && npm pack",
    "package_grid": "npm run build_grid && npm run npm_pack_grid",
    "package_lib": "npm run build_lib && npm run npm_pack_lib",
    "package": "npm run package_grid && npm run package_lib",
    "publish_grid": "npm run package_grid && cd dist/grid && npm publish",
    "publish_lib": "npm run package_lib && cd dist/library && npm publish",
    "publish": "npm run publish_grid && npm run publish_lib"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^11.2.8",
    "@angular/animations": "^11.2.9",
    "@angular/cdk": "^11.2.8",
    "@angular/common": "^11.2.9",
    "@angular/compiler": "^11.2.9",
    "@angular/core": "^11.2.9",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.2.9",
    "@angular/material": "^11.2.8",
    "@angular/platform-browser": "^11.2.9",
    "@angular/platform-browser-dynamic": "^11.2.9",
    "@angular/router": "^11.2.9",
    "@caiu/library": "^10.6.7",
    "@microsoft/signalr": "^3.1.14",
    "@ngrx/effects": "^11.1.0",
    "@ngrx/store": "^11.1.0",
    "ngx-color-picker": "^10.1.0",
    "rxjs": "~6.6.7",
    "tinymce": "^5.7.1",
    "tslib": "^2.2.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "ng-packagr": "^11.2.4",
    "tsickle": ">=0.38.1",
    "@angular-devkit/build-angular": "^14.2.8",
    "@angular/cli": "^11.2.8",
    "@angular/compiler-cli": "^11.2.9",
    "@angular/language-service": "^11.2.9",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.9.5",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.8.1",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
